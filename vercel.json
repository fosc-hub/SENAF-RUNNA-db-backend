{
    "builds": [
        {
            "src": "runna/runna/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "runna/runna/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "runna.runna.settings"
    }
}
